RECURRSION: TC O(2^n), SC O(n) -> auxillary stack space.
class Solution {
public:
    int fib(int n) {
        if(n==1) return 1;
        if(n==0) return 0;
        return fib(n-1) + fib(n-2);
    }
};
___________________________________________________________________________________________

MEMOIZATION: TC O(n), SC O(n) + O(n) -> dp array size and auxillary stack space.
#include<bits/stdc++.h>
class Solution {
public:
    int fib(int n) {
        vector<int> dp(n+1, -1);
        return func(n,dp);
    }

    int func(int n, vector<int>& dp){
        if(n==0) return 0;
        if(n==1) return 1;

        if(dp[n] != -1) return dp[n];
        return dp[n] = func(n-1,dp) + func(n-2,dp);
    }
};

_______________________________________________________________________________________________

TABULATION: TC O(n), SC O(n) auxillary stack space
#include<bits/stdc++.h>
class Solution {
public:
    int fib(int n) {
        if(n==0) return 0;
        if(n==1) return 1;
        vector<int> dp(n+1, -1);
        return func(n,dp);
    }
    int func(int n, vector<int>&dp){
        dp[0] = 0;
        dp[1] = 1;

        for(int i=2; i<=n; i++){
            dp[i] = dp[i-1] + dp[i-2];
        }
        return dp[n];
    }
};

_________________________________________________________________________________________________

SPACE OPTIMIZATION: TC O(n), SC O(1)
#include<bits/stdc++.h>
class Solution {
public:
    int fib(int n) {
        if(n<=1) return n;
        int prev = 1, prePrev = 0;

        for(int i=2; i<=n; i++){
           int curr = prev + prePrev;
            prePrev = prev;
            prev = curr;
        }
        return prev;

    }
};

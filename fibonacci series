
MEMOIZATION: TC O(n), SC O(n) + O(n) -> dp array size and auxillary stack space.
#include<bits/stdc++.h>
class Solution {
public:
    int fib(int n) {
        vector<int> dp(n+1, -1);
        return func(n,dp);
    }

    int func(int n, vector<int>& dp){
        if(n==0) return 0;
        if(n==1) return 1;

        if(dp[n] != -1) return dp[n];

        return dp[n] = func(n-1,dp) + func(n-2,dp);
    }
};
